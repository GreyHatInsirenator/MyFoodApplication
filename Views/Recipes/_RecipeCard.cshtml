@model IEnumerable<MyFoodApplication.Models.Recipes>

@foreach (var recipes in Model) 
{
    <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
        <div class="shadow rounded">
            <div class="card dishCard">
                <img src="@recipes.Image_url" class="card-img-top h-75" />
                <div class="card-body">
                    <p class="card-title">@recipes.Title</p>
                    <p class="card-text">@recipes.Publisher</p>
                </div>
            </div>
        </div>
    </div>
}
